{
    "name": "node-starter-kit",
    "version": "1.0.0",
    "description": "A Node.js project with Express, TypeScript, Prisma,Mysql ESLint, and Jest",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc",
        "postbuild": "npm run copy-swagger && npm run copy-locale",
        "copy-swagger": "copyfiles -u 1 src/config/swagger.json dist/",
        "copy-locale": "copyfiles -u 1 src/config/locales/* dist/",
        "lint:check": "eslint 'src/**/*.{js,ts}'",
        "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
        "format:check": "prettier --check 'src/**/*.{js,ts,json,md}'",
        "format:fix": "prettier --write 'src/**/*.{js,ts,json,md}'",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "seed": "ts-node prisma/seed.ts",
        "prisma:format": "prisma format",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:deploy": "prisma migrate deploy",
        "prepare": "echo skipping husky setup",
        "generate": "ts-node generate-files.ts",
        "postinstall": "npx playwright install chromium"
    },
    "dependencies": {
        "@fontsource-variable/public-sans": "^5.2.7",
        "@fontsource/public-sans": "^5.2.7",
        "@prisma/client": "^5.16.2",
        "@types/bcrypt": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/multer": "^1.4.11",
        "archiver": "^7.0.1",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "date-fns": "^3.6.0",
        "dotenv": "^16.6.1",
        "express": "^4.17.1",
        "express-validator": "^7.1.0",
        "fontkit": "^2.0.4",
        "fs-extra": "^11.2.0",
        "googleapis": "^159.0.0",
        "i18n": "^0.15.1",
        "inversify": "^6.0.2",
        "inversify-express-utils": "^6.4.6",
        "jsonwebtoken": "^9.0.2",
        "minimist": "^1.2.8",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.11.5",
        "nodemailer": "^7.0.6",
        "nodemon": "^3.1.4",
        "pdf-lib": "^1.17.1",
        "pdfkit": "^0.17.2",
        "playwright": "^1.55.1",
        "puppeteer": "^24.17.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "reflect-metadata": "^0.2.2",
        "sqlite3": "^5.1.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.13.1",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@types/archiver": "^6.0.3",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.13",
        "@types/fontkit": "^2.0.8",
        "@types/fs-extra": "^11.0.4",
        "@types/i18n": "^0.13.12",
        "@types/inquirer": "^9.0.7",
        "@types/jest": "^27.5.2",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.18.0",
        "@types/nodemailer": "^7.0.2",
        "@types/pdfkit": "^0.17.3",
        "@types/puppeteer": "^7.0.4",
        "@types/supertest": "^6.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^10.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.7",
        "inquirer": "^12.1.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.3.3",
        "prisma": "^5.16.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.4.4",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^5.9.2"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
            "prettier --write"
        ],
        "src/**/*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "engines": {
        "node": ">=14.0.0"
    }
}
